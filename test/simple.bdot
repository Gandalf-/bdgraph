#!/usr/local/bin/bdgraph
# 1,2,3 <- 4,5,6 == 1,2,3 each require 4,5,6
# 1,2,3 -> 4,5,6 == 1,2,3 each allow 4,5,6

   1: @Get out of bed
   2: Find socks
   3: Put on socks
   4: Find shoes
   5: Put on shoes
   6: Go to work
   7: win the lottery
   8: !buy a house
   9: buy a boat
  10: get in boat
  11: sail the world
  12: sail back home

options
  color_urgent
  color_complete
  color_next
  cleanup

dependencies
  1 -> 2,3,5,4
  3 <- 2
  5 <- 3,4
  6 <- 3,5
  7 -> 8,9
  7 <- 6
  9 -> 10,11
  11 <- 10
  12 <- 8,9,10
